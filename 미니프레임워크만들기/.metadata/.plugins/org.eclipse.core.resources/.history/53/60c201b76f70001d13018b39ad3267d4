		try {
			exam();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	private static void exam() throws Exception {
		//Dog d = new Dog(); //사실 이게 하고싶어 ㅠ
		
		Class<Dog> clz = Dog.class;
		clz.getDeclaredConstructor();
	}
}
